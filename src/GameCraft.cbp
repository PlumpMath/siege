			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add library="libSDL_draw" />
			<Add directory="C:\SDL\lib" />
		</Linker>
		<Unit filename="GC\GC.h" />
